{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "AzUrlShortener": {
      "appLocation": "Cloud5mins.ShortenerTools.TinyBlazorAdmin",
      "apiLocation": "Cloud5mins.ShortenerTools.Functions",
      "outputLocation": "bin/Debug/net6.0/wwwroot",
      "appBuildCommand": "dotnet publish -c Release -o bin",
      "apiBuildCommand": "dotnet publish -c Release",
      "run": "dotnet watch run"
      "networking": {
        "allowedIpRanges": [
          "194.171.164.1/22"
        ]
      }
    }
  }
}
